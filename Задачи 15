print('задание 1')
b = list(range(5, 25+1)) #создаем отрезок В
g = list(range(27, 41+1))
r = list(range(32, 50+1))
res = [] #список, который будет хранить отрезок D
for i in range(200):
	for j in range(200):
		d = list(range(i, j+1)) #создаем список D разных длин
		ok = True #ставим флаг, чтобы определить значения, при которых выражение истинно всегда
		for x in range(1,200):
			if not( ((x in d) and (x in b)) or ( ((x in g) <= (x in d)) and ((x not in d) <= (x not in r)) ) or ( (x not in d) and (x in b) ) ): 
			#переписали выражение и проотрицали его, чтобы определить, когда выражение ложно
				ok = False #меняем значение флага -> выражение ложно и выбранный отрезок не подходит
				break
		if ok and (len(res) == 0 or len(d)<len(res)): 
		#проверяем, что все Х не зануляют выражение и находим минимальную длину отрезка через сравнение
			res = d
print(res[0], res[-1]) #выводим начало и конец получившегося отрезка'''

print('задание 2')
for a in range(1,10000): #запускаем значения для А
	ok = True #ставим флаг, чтобы определить значения, при которых выражение истинно всегда
	for x in range(1000): #перебираем значения для Х
		for y in range(1000):
			if not( ((21*y + 8*x) <=a) or ((1785*x + 969*y +3) > 33918) ): 
			#переписали выражение и проотрицали его, чтобы определить, когда выражение ложно
				ok = False #меняем значение флага -> выражение ложно и выбранное число не подходит
				break
		if not ok: #выходим из цикла Х
			break
	if ok:#проверяем, что все Х не зануляют выражение
		for x in range(90): #диапазон для ширины
		    for y in range(60): #диапазон для высоты
		        if 8*x + 21*y == a and 8*x<=90 and 21*y<=60 : #проверка на вместимость на холст
		            print(a, x, y)
		            exit()
print(0)

print('задание 3')
for a in range(1,1000):#запускаем значения для А
	ok = True #ставим флаг, чтобы определить значения, при которых выражение истинно всегда
	for x in range(1,1000): #перебираем значения для Х
		if not( (26%a !=0) or (x%26 !=0)  and (x%39==0) ):
			#переписали выражение и проотрицали его, чтобы определить, когда выражение ложно
			ok = False #меняем значение флага -> выражение ложно и выбранное число не подходит
			break
	if ok and a<10:#проверяем, что все Х не зануляют выражение 
		print(a)
		break #завершить программу на минимальном значении
